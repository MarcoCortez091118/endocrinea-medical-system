{
  "routes": [
    {
      "route": "/static/*",
      "headers": {
        "Cache-Control": "public, max-age=86400"
      }
    },
    {
      "route": "/favicon.ico",
      "headers": {
        "Cache-Control": "public, max-age=86400"
      }
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ]
}
